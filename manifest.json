{
   "manifest_version": 2,
   "background": {
      "page": "background/background.html"
   },
   "content_scripts": [
      {
         "all_frames": true,
         "js": [ "specific/sub_frame.js" ],
         "matches": [ "<all_urls>" ],
         "run_at": "document_start"
      },
      {
         "all_frames": false,
         "js": [
           "js/REWRITE.js"
         ],
         "matches": [ "*://*.google.com/search*", "*://*.google.com/webhp*" ],
         "run_at": "document_start"
      },
      {
         "all_frames": false,
         "js": [
           "js/scrollToFirstResult.js"
         ],
         "matches": [ "*://*.google.com/search*", "*://*.google.com/webhp*" ],
         "run_at": "document_end"
      },
      {
         "all_frames": false,
         "js": [
           "node_modules/extension-include/extension-include.js",
           "mod/messaging.js",
           "mod/cs_analytics.js",
           "mod/neif.js",
           "mod/preview.js",
           "mod/fresh.js",
           "misc/start.js"
         ],
         "matches": [ "*://*.NOTGOOGLEgoogle.com/search*", "*://*.NOTGOOGLEgoogle.com/webhp*" ],
         "run_at": "document_start"
      },
      {
         "all_frames": true,
         "js": [ "specific/youtube.js" ],
         "matches": [ "*://*.youtube.com/*" ],
         "run_at": "document_start"
      }
   ],
   "description": "Scout - Google like a god",
   "icons":
   {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   "web_accessible_resources": ["css/SCOUT-CSS.css", "css/REWRITE.css"],
   "incognito": "spanning",
   "minimum_chrome_version": "20",
   "update_url": "http://thescoutapp.com/extension/update.xml",
   "name": "Scout 4.0 Eros",
   "homepage_url": "http://TheScoutApp.com",
   "permissions": [
     "<all_urls>",
     "chrome://favicon/",
     "clipboardWrite",
     "contextMenus",
     "cookies",
     "idle",
     "notifications",
     "unlimitedStorage",
     "storage",
     "webRequest",
     "webRequestBlocking",
     "declarativeWebRequest"
   ],
   "version": "4.0"
}